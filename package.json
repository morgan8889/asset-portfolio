{
  "name": "portfolio-tracker",
  "version": "0.1.0",
  "private": true,
  "description": "A modern, privacy-first financial portfolio tracking application with interactive visualizations",
  "keywords": [
    "portfolio",
    "finance",
    "trading",
    "investment",
    "nextjs",
    "typescript"
  ],
  "author": "Portfolio Tracker Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/portfolio-tracker.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,tsx,json,css,md}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:run && npm run test:e2e",
    "prepare": "husky install"
  },
  "dependencies": {

    "lucide-react": "^0.400.0",
    "next": "14.2.35",
    "next-themes": "^0.3.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^2.2.2",
    "react-hook-form": "^7.63.0",
    "react-resizable": "^3.1.3",
    "react-window": "^1.8.8",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.3.0",
